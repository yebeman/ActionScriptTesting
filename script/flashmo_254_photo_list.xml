<?xml version="1.0" encoding="utf-8"?>
<photos>

	<config 
		hover_effect="true"
		auto_play="true" 
		auto_play_duration="6" 
		grid_row="3" 
		grid_column="9" 
		tween_duration="0.7" 
		tween_delay="0.03">
	</config>
	

	
	<photo>
		<filename>photos/ferrari.jpg</filename>
		<url>http://www.ferrari.com</url> 
		<target>_blank</target> 
		<transition flow="in" direction="left"></transition>
	</photo>
	
		<photo>
		<filename>photos/mcdonalds.jpg</filename>
		<url>http://www.mcdonalds.com</url> 
		<target>_parent</target> 
		<transition flow="out" direction="right"></transition>
	</photo>
	
	<photo>
		<filename>photos/starbucks.jpg</filename>
		<url>http://www.starbucks.com</url> 
		<target>_self</target> 
		<transition flow="out" direction="down" rotation="-180"></transition>
	</photo>
	
	<photo>
		<filename>photos/avengers.png</filename>
		<url>http://marvel.com/universe/Avengers</url> 
		<target>_parent</target>
		<transition flow="in" direction="up"></transition>
	</photo>
	
	<photo>
		<filename>photos/cocacola.jpg</filename>
		<url>http://www.coca-cola.com</url> 
		<target>_blank</target>
		<transition flow="in" direction="center" rotation="180"></transition>
	</photo>

</photos>